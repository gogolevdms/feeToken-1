    "coverage": "^0.4.1",
    "dotenv": "^11.0.0",
    "hardhat": "^2.8.2",
    "hardhat-deploy": "^0.9.24",
    "prettier": "^2.5.1",
    "solhint": "^3.3.6",
    "solidity-coverage": "^0.7.17"
  },
  "devDependencies": {
    "@nomiclabs/hardhat-ethers": "^2.0.4",
    "@nomiclabs/hardhat-waffle": "^2.0.0",
